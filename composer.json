{
  "name": "antkaz/yii2-iview",
  "description": "The iView extension for the Yii framework",
  "homepage": "https://github.com/antkaz/yii2-iview",
  "type": "yii2-extension",
  "license": "MIT",
  "keywords": [
    "yii2",
    "iview"
  ],
  "support": {
    "source": "https://github.com/antkaz/yii2-iview",
    "issues": "https://github.com/antkaz/yii2-iview/issues"
  },
  "authors": [
    {
      "name": "Anton Kazarinov",
      "email": "askazarinov@gmail.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.14",
    "antkaz/yii2-vue": "~1.0",
    "npm-asset/iview": "~2.12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5"
  },
  "config": {
    "fxp-asset": {
      "enabled": false
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "autoload": {
    "psr-4": {
      "antkaz\\iview\\": "src"
    }
  }
}
